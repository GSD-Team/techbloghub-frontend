{
  "extends": [
    "next/core-web-vitals",
    "airbnb",
    "airbnb/hooks",
    "airbnb-typescript",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "root": true,
  "tsconfigRootDir": "./",
  "rules": {
    "no-return-assign": "off",
    "react/no-array-index-key": "off",
    "react/jsx-props-no-spreading": "off"
  }
}